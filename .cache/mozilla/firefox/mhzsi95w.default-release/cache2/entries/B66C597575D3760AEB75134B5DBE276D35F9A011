		<generic>
			<label>short-time-display-without-seconds</label>
			<!-- 1 - hours (24 hours day format)
				 2 - minutes
				 3 - hours (12 hours day format)
				 4 - hours extension (for 12 hours day format [am|pm])

				 %s : string
				 %02d : digit with left 0 padding (max 2 digits)
				 3$ : Means put the 3rd value (and not the first one here)
				-->
			<ar>%3$02d.%2$02d%4$s</ar>
			<de>%02d:%02d</de>
			<en>%3$02d.%2$02d%4$s</en>
			<es>%02d:%02d</es>
			<fr>%02d:%02d</fr>
			<ko>%02d:%02d</ko>
			<ja>%02d:%02d</ja>
			<th>%02d:%02d</th>
			<ru>%1$02d:%2$02d</ru>
			<id>%02d:%02d</id>
			<it>%02d:%02d</it>
			<nl>%02d:%02d uur</nl>
			<pt>%02d:%02d</pt>
			<pl>%02d:%02d</pl>
			<zh_CN>%02d:%02d</zh_CN>
			<zh_TW>%02d:%02d</zh_TW>
		</generic>
		<generic>
			<label>full-date-display</label>
			<!-- 1 - year
				 2 - month (1-12)
				 3 - name of month
				 4 - day of month (1-31)
				 5 - name of day
				 6 - hours (24 hours day format)
				 7 - minutes
				 8 - hours (12 hours day format)
				 9 - hours extension (for 12 hours day format [am|pm])

				 %s : string
				 %02d : digit with left 0 padding (max 2 digits)
				 3$ : Means put the 3rd value (and not the first one here)
				 -->
			<ar>%5$s %4$02d %3$s %1$4d %8$02d.%7$02d%9$s</ar>
			<de>%5$s %4$02d. %3$s %1$4d %6$02d:%7$02d</de>
			<en>%5$s %4$02d %3$s %1$4d %8$02d.%7$02d%9$s</en>
			<es>El %5$s %4$02d de %3$s de %1$4d a las %6$02d:%7$02d</es>
			<fr>%5$s %4$02d %3$s %1$4d %6$02d:%7$02d</fr>
			<ko>%5$s %4$02d %3$s %1$4d %6$02d:%7$02d</ko>
			<ja>%5$s %4$02d %3$s %1$4d %6$02d:%7$02d</ja>
			<th>%5$s %4$02d %3$s %1$4d %6$02d:%7$02d</th>
			<ru>%4$02d %3$s %1$4d %5$s %6$02d:%7$02d</ru>
			<id>%5$s %4$02d %3$s %1$4d %6$02d:%7$02d</id>
			<it>%5$s %4$02d %3$s %1$4d %6$02d:%7$02d</it>
			<nl>%5$s %4$02d %3$s %1$4d, %6$02d:%7$02d uur</nl>
			<pt>%5$s %4$02d %3$s %1$4d %6$02d:%7$02d</pt>
			<pl>%5$s %4$02d %3$s %1$4d %6$02d:%7$02d</pl>
			<zh_CN>%1$4d %3$s %4$02d号, %5$s, %6$02d:%7$02d</zh_CN>
			<zh_TW>%1$4d %3$s %4$02d号, %5$s, %6$02d:%7$02d</zh_TW>
		</generic>

		<generic>
